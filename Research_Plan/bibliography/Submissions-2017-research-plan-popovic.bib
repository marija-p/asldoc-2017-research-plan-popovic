@article{Hitz2015,
abstract = {Autonomous mobile robots are increasingly employed to take measurements for environ- mental monitoring, but planning informative, measurement-rich paths through large three- dimensional (3D) environments is still challenging. The problem of planning such paths is known as the informative path planning (IPP) problem, and has been shown to be non- deterministic polynomial-time (NP)-hard. Existing algorithms focus on providing guaran- tees on suboptimal solutions, but do not scale well to large problem instances. In this paper, we introduce a novel IPP algorithm that uses an evolutionary strategy (ES) technique to optimize a parametrized path in continuous space, which is subject to various constraints regarding path budgets and motion capabilities of an autonomous mobile robot. Moreover, we introduce a replanning scheme to adapt the planned paths according to information gained from measurements taken in situ during a data collection mission. When compared to opti- mal paths on discrete graphs, our method provides competitive results at significantly lower computation times and memory requirements. Our results show that planning in continuous space can provide solutions which outperform the optimum on a discrete graph. The proposed replanning scheme enables to build models with up to 25{\{}{\%}{\}} lower uncertainty within an (initially unknown) areas of interest. After presenting theoretical results, we tailored the proposed algorithms for data collection using an autonomous surface vessel (ASV) within the context of an ecological study, during which the method was validated through three field deployments on Lake Zurich, Switzerland. Spatiotemporal variations are shown over a period of three months and an area of 350 m × 350 m × 13 m. Whereas our theoretical solution can be applied to multiple applications, our field results specifically highlight the effectiveness of our planner for monitoring toxic cyanobacteria populations in a pre-alpine lake, and for identifying hot-spots within the cyanobacterial distribution.},
annote = {under review},
author = {Hitz, Gregory and Galceran, Enric and Garneau, Marie-{\`{E}}ve and Pomerleau, Fran{\c{c}}ois and Siegwart, Roland},
file = {:home/marija/Documents/Mendeley Desktop/Environmental Practice/Bayesian Optimisation for Informative Continuous Path Planning/Dunbabin et al.{\_}Bayesian Optimisation for Informative Continuous Path Planning(3).pdf:pdf},
keywords = {3D,asl,autonomous,budget,eth,evolutionary,flourish,gathering,information,informative,limno,limnobotics,measurement,path,planning,strategy},
mendeley-tags = {3D,asl,autonomous,budget,eth,evolutionary,flourish,gathering,information,informative,limno,limnobotics,measurement,path,planning,strategy},
title = {{Adaptive Continuous-Space Informative Path Planning for Online Environmental Monitoring}},
year = {2016}
}
@article{Binney2013,
abstract = {We introduce a graph-based informative path planning algorithm for a mobile robot which explicitly handles time. The objective function must be submodular in the samples taken by the robot, and the samples obtained are allowed to depend on the time at which the robot visits each location. Using a submodular objective function allows our algorithm to handle problems with diminishing returns, e.g. the case when taking a sample provides less utility when other nearby points have already been sampled. We give a formal description of this framework wherein an objective function that maps the path of the robot to the set of samples taken is defined. We also show how this framework can handle the case in which the robot takes samples along the edges of the graph. A proof of the approximation guarantee for the algorithm is given. Finally, quantitative results are shown for three problems: one simple example with a known Gaussian process model, one simulated example for an underwater robot planning problem using data from a well-known ocean modeling system, and one field experiment using an autonomous surface vehicle (ASV) measuring wireless signal strength on a lake.},
author = {Binney, Johnathan and Krause, Andreas and Sukhatme, Gaurav. S.},
doi = {10.1177/0278364913488427},
file = {:home/marija/Documents/Mendeley Desktop/The International Journal of Robotics Research/Optimizing waypoints for monitoring spatiotemporal phenomena/Binney, Krause, Sukhatme{\_}Optimizing waypoints for monitoring spatiotemporal phenomena.pdf:pdf},

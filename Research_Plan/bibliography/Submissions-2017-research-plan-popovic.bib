journal = {The International Journal of Robotics Research},
keywords = {asv,autonomous,function,graph-based,information,informative,ipp,mapping,marine,marine robotics,mobile,objective,path,planning,robot,robotics,sensor networks,submodular,vehicle},
mendeley-tags = {asv,autonomous,function,graph-based,information,informative,ipp,mapping,marine,marine robotics,mobile,objective,path,planning,robot,robotics,sensor networks,submodular,vehicle},
